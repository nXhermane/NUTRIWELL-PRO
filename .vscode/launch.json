   "version": "0.2.0",
   "configurations": [
      {
         "type": "expo",
         "request": "attach",
         "name": "Debug Expo app",
         "projectRoot": "${workspaceFolder}",
         "bundlerPort": "8081",
         "bundlerHost": "127.0.0.1"
      },
      {
         "name": "Debug Expo Web - Experimental",
         "request": "launch",
         "type": "reactnative",
         "cwd": "${workspaceFolder}",
         "platform": "expoweb",
         "browserTarget": "chrome",
         "url": "http://localhost:8081"
      }
   ]
}
